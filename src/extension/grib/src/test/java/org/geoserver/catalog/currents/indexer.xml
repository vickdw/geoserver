<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<Indexer>
    <domains>
      <domain name="time">
         <attributes><attribute>time</attribute></attributes>
      </domain>
    
    </domains>
    <schemas>
        <schema name="default" >
            <attributes>the_geom:Polygon,location:String,imageindex:Integer,time:java.util.Date</attributes>
        </schema>
    </schemas>
    <coverages>
        <coverage>
            <name>u-component_of_current_surface</name>
            <schema ref="default"></schema>
            <domains>
              <domain ref="time" />
            </domains>
        </coverage>
        <coverage>
            <name>v-component_of_current_surface</name>
            <schema ref="default"></schema>
            <domains>
              <domain ref="time" />
            </domains>
        </coverage>
       
    </coverages>
    <collectors>
       </collectors>
    <parameters>
        <parameter name="AuxiliaryFile" value="_currents.xml" />
        <parameter name="AbsolutePath" value="false" />
    </parameters>
</Indexer>